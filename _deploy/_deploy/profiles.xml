<?xml version="1.0" encoding="UTF-8"?>
<!-- List of profiles available to be assigned to hosts. -->
<profiles:profiles xmlns:profiles="http://www.wpkg.org/profiles"
	xmlns:wpkg="http://www.wpkg.org/wpkg" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.wpkg.org/profiles xsd/profiles.xsd ">
	<profile id="default">
		<package package-id="ruloggio" />		
		<package package-id="7zip" />
		<package package-id="cdex" />
	</profile>
	<profile id="edu">
		<depends profile-id="default" />
		<package package-id="bibbia" />
		<package package-id="artweaverplugins" />
		<package package-id="autoit" />		
		<package package-id="ccleaner" />
		<package package-id="takesnapshot" />
	</profile>
	<profile id="home">
		<depends profile-id="default" />
		<package package-id="ccleaner" />
	</profile>
	<profile id="domain">
		<depends profile-id="default" />
		<package package-id="atlogon" />
		<package package-id="firewalloff" />
		<package package-id="rdpon" />
	</profile>
	<profile id="other">
		<depends profile-id="edu" />
		<package package-id="firewalloff" />
	</profile>
</profiles:profiles>
